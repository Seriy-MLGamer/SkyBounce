length=2048
camera_x=0
camera_y=0
camera_scale=.5
x=WINDOW_XSIZE div 2/camera_scale-length+camera_x
fbox((x-camera_x)*camera_scale, -camera_y*camera_scale, length*camera_scale, 16*camera_scale, GREEN)
frame()
while 1
{
	while get_event()
	{
		if EVT[EVT_TYPE]==EVT_QUIT
		{
			halt
		}
	}
	sleep(10)
}